{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle": 0,
        "func-style": 0,
        "indent": "off",
        "jsx-quotes": ["error", "prefer-single"],
        "no-use-before-define": 0,
        "semi": "off",

        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": [2, 4],
        "react/prop-types": 0
    }
}